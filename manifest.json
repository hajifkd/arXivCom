{
  "name": "arXivCom",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Add comment feature to arXiv.org",
  "content_scripts" : [
    {
      "matches": ["http://arxiv.org/abs/*"],
      "js": ["libs/jquery-2.2.1.min.js", "libs/hogan-3.0.1.js", "arXiv.js"]
    }
  ],
  "web_accessible_resources": ["css/arXiv_style.css", "templates/*"],
  "permissions": ["http://192.168.56.100:8000/*"]
}